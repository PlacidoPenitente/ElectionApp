<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:ElectionApp.ViewModel"
                    xmlns:view="clr-namespace:ElectionApp.View">
    <DataTemplate DataType="{x:Type viewmodel:LoginPageViewModel}">
        <view:LoginPageView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:LoginBannerViewModel}">
        <view:LoginBannerView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:WorkspaceViewModel}">
        <view:WorkspaceView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:WorkspaceBannerViewModel}">
        <view:WorkspaceBannerView/>
    </DataTemplate>
</ResourceDictionary>